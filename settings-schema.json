{"title": "settings"
,"description": "JSON schema for paneled-outlier-explorer configuration"
,"version": "1.0.1"
,"type": "object"
,"properties":
    {"measure_col":
        {"title": "Measure"
        ,"description": "measure variable name"
        ,"default": "TEST"
        ,"type": "string"}
    ,"time_cols":
        {"title": "Time"
        ,"description": "an array of each time variable and associated metadata"
        ,"type": "array"
        ,"items":
            {"type": "object"
            ,"properties":
                {"value_col":
                    {"title": "Time variable name"
                    ,"type": "string"}
                ,"type":
                    {"title": "Time variable data type"
                    ,"type": "string"}
                ,"label":
                    {"title": "Time variable label"
                    ,"type": "string"}
                ,"rotate_tick_labels":
                    {"title": "Rotate x-axis tick labels?"
                    ,"type": "boolean"}
                ,"vertical_space":
                    {"title": "X-axis vertical space"
                    ,"type": "number"}}}
        ,"default":
            [
                {"value_col": "DY"
                ,"type": "linear"
                ,"label": "Study Day"
                ,"rotate_tick_labels": false
                ,"vertical_space": 0}
            ,
                {"value_col": "VISIT"
                ,"type": "ordinal"
                ,"label": "Visit"
                ,"rotate_tick_labels": true
                ,"vertical_space": 100}
            ,
                {"value_col": "VISITN"
                ,"type": "ordinal"
                ,"label": "Visit Number"
                ,"rotate_tick_labels": false
                ,"vertical_space": 0}
            ]}
    ,"value_col":
        {"title": "Result"
        ,"description": "result variable name"
        ,"default": "STRESN"
        ,"type": "string"}
    ,"id_col":
        {"title": "Identifier"
        ,"description": "identifier variable name"
        ,"default": "USUBJID"
        ,"type": "string"}
    ,"unit_col":
        {"title": "Unit"
        ,"description": "unit variable name"
        ,"default": "STRESU"
        ,"type": "string"}
    ,"lln_col":
        {"title": "Lower Limit of Normal"
        ,"description": "lower limit of normal variable name"
        ,"default": "STNRLO"
        ,"type": "string"}
    ,"uln_col":
        {"title": "Upper Limit of Normal"
        ,"description": "upper limit of normal variable name"
        ,"default": "STNRHI"
        ,"type": "string"}
    ,"measures":
        {"title": "Measure list"
        ,"description": "list of initially displayed measures"
        ,"type": "array"
        ,"items":
            {"type": "string"}}
    ,"filters":
        {"title": "Filters"
        ,"description": "an array of each filter variable and associated metadata"
        ,"type": "array"
        ,"items":
            {"type": "object"
            ,"properties":
                {"value_col":
                    {"title": "Variable name"
                    ,"type": "string"}
                ,"label":
                    {"title": "Variable label"
                    ,"type": "string"}}}}
    }
}
