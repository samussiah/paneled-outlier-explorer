{
  "name": "paneled-outlier-explorer",
  "version": "1.0.0",
  "description": "Paneled line charts of data points over time with point and line brushing",
  "main": "./src/index.js",
  "scripts": {
    "build": "npm run format && npm run bundle && prettier --single-quote --tab-width=4 --print-width=100 --write ./build/paneledOutlierExplorer.js",
    "bundle": "rollup -c",
    "format": "prettier --single-quote --tab-width=4 --print-width=100 --write \"./src/**/*.js\"",
    "watch": "rollup -c -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RhoInc/paneled-outlier-explorer.git"
  },
  "keywords": [
    "paneled",
    "outlier",
    "explorer",
    "longitudinal",
    "labs",
    "vital",
    "signs"
  ],
  "author": "Rho, Inc.",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/RhoInc/paneled-outlier-explorer/issues"
  },
  "homepage": "https://github.com/RhoInc/paneled-outlier-explorer#readme",
  "dependencies": {
      "d3": "~3",
      "webcharts": "~1"
  },
  "devDependencies": {
    "babel-cli": "^6.0.0",
    "babel-preset-es2015-rollup": "^1.1.1",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.4.0",
    "rollup-watch": "~3.2.2",
    "prettier":"~1"
  }
}
